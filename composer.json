{
    "name": "marellocommerce/marello",
    "description": "Marello",
    "homepage": "https://github.com/marellocommerce/marello.git",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Madia B.V.",
            "homepage": "http://www.marello.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Marello\\Bundle": "src/"
         }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        }
